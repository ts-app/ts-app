{
  "extends": [
    "tslint-config-standard",
    "tslint-etc",
    "rxjs-tslint-rules"
  ],
  "rules": {
    "no-unused-variable": false,
    "no-unused-declaration": true,
    "throw-error": true,
    "rxjs-finnish": {
      "options": [
        {
          "functions": true,
          "methods": false,
          "parameters": true,
          "properties": true,
          "variables": true
        }
      ],
      "rxjs-no-do": true
    }
  }
}
